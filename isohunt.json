{
    "name":"Isohunt",
    "baseUrl": "http://isohunt.to",
    "pattern":"/torrents/?ihq=[text]&Torrent_page=[page]&iht=[category]",
    "pageRules":
    {
	"start":0,
	"step":40,
	"maxItems":40
    },
    "headers":
    {
	"User-Agent":"'Mozilla/5.0'"
    },
    "categories":
    {
	"All":"0",
	"Anime":"1",
	"Software":"2",
	"Games":"3",
	"Movies":"5",
	"Music":"6",
	"Other":"7",
	"Tv":"8",
	"Books":"9"
    },
    "ops":
    {
	"titles":
	{
	    "xpath":"//*[@id=\"serps\"]/table/tbody/tr[not(td[1]/text()='Sponsored')]/td[2]/div/a/span",
	    "container":"text"
	},
	"sizes":
	{
	    "xpath":"//*[@id=\"serps\"]/table/tbody/tr[not(td[1]/text()='Sponsored')]/td[6]",
	    "container":"text"
	},
	"categories":
	{
	    "xpath":"//*[@id=\"serps\"]/table/tbody/tr[not(td[1]/text()='Sponsored')]/td[2]/div/em/span/a",
	    "container":"text"
	},
	"seeders":
	{
	    "xpath":"//*[@id=\"serps\"]/table/tbody/tr[not(td[1]/text()='Sponsored')]/td[7]",
	    "container":"text"
	},
	"leechers":
	{
	    "xpath":"//*[@id=\"serps\"]/table/tbody/tr[not(td[1]/text()='Sponsored')]/td[8]",
	    "container":"text"
	},
	"magnets":
	{
	    "xpath":"//*[@id=\"serps\"]/table/tbody/tr[not(td[1]/text()='Sponsored')]/td[2]/div/a",
	    "container":"href",
	    "crawler":{"xpath":"//*[@class=\"torr-down-buttons\"]/a[2]"}
	}
    }
}
